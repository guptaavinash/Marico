{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Marico-Astix-Tas-Integration"
		},
		"AstixProdServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AstixProdServer'"
		},
		"MaricoAstixTASDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MaricoAstixTASDB'"
